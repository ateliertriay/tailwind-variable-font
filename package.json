{
  "name": "tailwind-variable-font",
  "version": "1.0.0",
  "description": "Use variable fonts with Tailwind CSS classes",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin main && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm run format && npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ateliertriay/tailwind-variable-font.git"
  },
  "bugs": {
    "url": "https://github.com/ateliertriay/tailwind-variable-font/issues"
  },
  "homepage": "https://github.com/ateliertriay/tailwind-variable-font#readme",
  "keywords": [
    "create-tailwind-plugin",
    "tailwind-css-plugin",
    "tailwindcss-plugin",
    "tailwindcss",
    "plugin"
  ],
  "author": "Mathieu Triay",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.46.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^27.2.0",
    "husky": "^8.0.0",
    "jest": "^29.6.0",
    "jest-matcher-css": "^1.1.0",
    "lint-staged": "^13.2.0",
    "lodash": "^4.17.20",
    "postcss": "^8.1.9",
    "standard-version": "^9.1.0",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "lodash.permutations": "^1.0.0"
  }
